<?xml version="1.0" encoding="UTF-8"?>
<interface domain="gnome-pomodoro">
  <template class="PomodoroInstallExtensionDialog" parent="GtkMessageDialog">
<!--    <property name="can-focus">False</property>-->
<!--    <property name="can-default">True</property>-->
<!--    <property name="has-default">True</property>-->
<!--    <property name="receives-default">True</property>-->
    <property name="modal">True</property>
    <property name="destroy-with-parent">True</property>
<!--    <property name="type-hint">dialog</property>-->
    <action-widgets>
      <action-widget response="0">cancel_button</action-widget>
      <action-widget response="2">manage_extensions_button</action-widget>
      <action-widget response="3">report_button</action-widget>
      <action-widget response="1">close_button</action-widget>
      <action-widget response="1">done_button</action-widget>
    </action-widgets>
    <child internal-child="vbox">
      <object class="GtkBox">
<!--        <property name="can-focus">False</property>-->
<!--        <property name="orientation">vertical</property>-->
<!--        <property name="spacing">2</property>-->
        <child internal-child="action_area">
          <object class="GtkBox" id="action_area">
<!--            <property name="can-focus">False</property>-->
<!--            <property name="homogeneous">True</property>-->
<!--            <property name="layout-style">expand</property>-->
            <child>
              <object class="GtkButton" id="cancel_button">
                <property name="label" translatable="yes">_Cancel</property>
                <property name="name">cancel</property>
                <property name="can-focus">True</property>
                <property name="receives-default">True</property>
                <property name="use-underline">True</property>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="fill">True</property>
                <property name="position">0</property>
              </packing>
            </child>
            <child>
              <object class="GtkButton" id="manage_extensions_button">
                <property name="label" translatable="yes">_Manage extensions</property>
                <property name="name">manage-extensions</property>
                <property name="can-focus">True</property>
                <property name="receives-default">False</property>
                <property name="use-underline">True</property>
              </object>
              <packing>
                <property name="expand">True</property>
                <property name="fill">True</property>
                <property name="position">1</property>
              </packing>
            </child>
            <child>
              <object class="GtkButton" id="report_button">
                <property name="label" translatable="yes">_Report issue</property>
                <property name="name">report-issue</property>
                <property name="can-focus">True</property>
                <property name="receives-default">False</property>
                <property name="use-underline">True</property>
              </object>
              <packing>
                <property name="expand">True</property>
                <property name="fill">True</property>
                <property name="position">2</property>
              </packing>
            </child>
            <child>
              <object class="GtkButton" id="close_button">
                <property name="label" translatable="yes">_Close</property>
                <property name="name">close</property>
                <property name="can-focus">True</property>
                <property name="receives-default">True</property>
                <property name="use-underline">True</property>
              </object>
              <packing>
                <property name="expand">True</property>
                <property name="fill">True</property>
                <property name="position">3</property>
              </packing>
            </child>
            <child>
              <object class="GtkButton" id="done_button">
                <property name="label" translatable="yes">_Done</property>
                <property name="name">done</property>
                <property name="can-focus">True</property>
                <property name="receives-default">True</property>
                <property name="use-underline">True</property>
                <style>
                  <class name="suggested-action"/>
                </style>
              </object>
              <packing>
                <property name="expand">True</property>
                <property name="fill">True</property>
                <property name="position">4</property>
              </packing>
            </child>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">False</property>
            <property name="position">0</property>
          </packing>
        </child>
        <child>
          <object class="GtkStack" id="stack">
            <property name="can-focus">False</property>
            <property name="vhomogeneous">False</property>
            <property name="transition-type">crossfade</property>
            <child>
              <object class="GtkBox">
                <property name="can-focus">False</property>
                <property name="margin-start">24</property>
                <property name="margin-end">24</property>
                <property name="margin-top">24</property>
                <property name="margin-bottom">48</property>
                <property name="orientation">vertical</property>
                <property name="spacing">10</property>
                <child>
                  <object class="GtkSpinner" id="spinner">
                    <property name="height-request">32</property>
                    <property name="can-focus">False</property>
                    <property name="active">True</property>
                  </object>
                  <packing>
                    <property name="expand">False</property>
                    <property name="fill">True</property>
                    <property name="position">0</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkLabel">
                    <property name="can-focus">False</property>
                    <property name="label" translatable="yes">Installing extensionâ€¦</property>
                  </object>
                  <packing>
                    <property name="expand">False</property>
                    <property name="fill">True</property>
                    <property name="position">1</property>
                  </packing>
                </child>
              </object>
              <packing>
                <property name="name">in-progress</property>
              </packing>
            </child>
            <child>
              <object class="GtkBox">
                <property name="can-focus">False</property>
                <property name="margin-start">24</property>
                <property name="margin-end">24</property>
                <property name="margin-top">24</property>
                <property name="margin-bottom">48</property>
                <property name="orientation">vertical</property>
                <property name="spacing">10</property>
                <child>
                  <object class="GtkLabel">
                    <property name="can-focus">False</property>
                    <property name="label" translatable="yes">Extension has been installed</property>
                    <property name="max-width-chars">60</property>
                    <style>
                      <class name="title"/>
                    </style>
                  </object>
                  <packing>
                    <property name="expand">False</property>
                    <property name="fill">False</property>
                    <property name="position">0</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkLabel">
                    <property name="can-focus">False</property>
                    <property name="margin-bottom">2</property>
                    <property name="label" translatable="yes">If you change your mind, you can uninstall it using &lt;i&gt;Extensions&lt;/i&gt; app.</property>
                    <property name="use-markup">True</property>
                    <property name="wrap">True</property>
                    <property name="max-width-chars">60</property>
                  </object>
                  <packing>
                    <property name="expand">False</property>
                    <property name="fill">True</property>
                    <property name="position">1</property>
                  </packing>
                </child>
              </object>
              <packing>
                <property name="name">success</property>
                <property name="position">1</property>
              </packing>
            </child>
            <child>
              <object class="GtkBox">
                <property name="can-focus">False</property>
                <property name="margin-start">24</property>
                <property name="margin-end">24</property>
                <property name="margin-top">24</property>
                <property name="margin-bottom">48</property>
                <property name="orientation">vertical</property>
                <property name="spacing">10</property>
                <child>
                  <object class="GtkLabel">
                    <property name="can-focus">False</property>
                    <property name="label" translatable="yes">Could not enable the extension</property>
                    <property name="max-width-chars">60</property>
                    <style>
                      <class name="title"/>
                    </style>
                  </object>
                  <packing>
                    <property name="expand">False</property>
                    <property name="fill">False</property>
                    <property name="position">0</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkLabel">
                    <property name="can-focus">False</property>
                    <property name="margin-bottom">2</property>
                    <property name="label" translatable="yes">Extension has been installed but there is trouble enabling it. You may try logging out.</property>
                    <property name="use-markup">True</property>
                    <property name="wrap">True</property>
                    <property name="max-width-chars">60</property>
                  </object>
                  <packing>
                    <property name="expand">False</property>
                    <property name="fill">True</property>
                    <property name="position">1</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkExpander">
                    <property name="can-focus">True</property>
                    <property name="margin-start">24</property>
                    <property name="margin-end">24</property>
                    <property name="resize-toplevel">True</property>
                    <child>
                      <object class="GtkTextView" id="error_enabling_textview">
                        <property name="can-focus">True</property>
                        <property name="editable">False</property>
                        <property name="monospace">True</property>
                      </object>
                    </child>
                    <child type="label">
                      <object class="GtkLabel">
                        <property name="can-focus">False</property>
                        <property name="label" translatable="yes">Technical Details</property>
                      </object>
                    </child>
                  </object>
                  <packing>
                    <property name="expand">False</property>
                    <property name="fill">True</property>
                    <property name="position">2</property>
                  </packing>
                </child>
              </object>
              <packing>
                <property name="name">error-enabling</property>
                <property name="position">2</property>
              </packing>
            </child>
            <child>
              <object class="GtkBox">
                <property name="can-focus">False</property>
                <property name="margin-start">24</property>
                <property name="margin-end">24</property>
                <property name="margin-top">24</property>
                <property name="margin-bottom">48</property>
                <property name="orientation">vertical</property>
                <property name="spacing">10</property>
                <child>
                  <object class="GtkLabel">
                    <property name="can-focus">False</property>
                    <property name="label" translatable="yes">Could not install the extension</property>
                    <property name="max-width-chars">60</property>
                    <style>
                      <class name="title"/>
                    </style>
                  </object>
                  <packing>
                    <property name="expand">False</property>
                    <property name="fill">False</property>
                    <property name="position">0</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkLabel">
                    <property name="can-focus">False</property>
                    <property name="margin-bottom">2</property>
                    <property name="label" translatable="yes">There was an issue installing the extension.</property>
                    <property name="use-markup">True</property>
                    <property name="wrap">True</property>
                    <property name="max-width-chars">60</property>
                  </object>
                  <packing>
                    <property name="expand">False</property>
                    <property name="fill">True</property>
                    <property name="position">1</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkExpander">
                    <property name="can-focus">True</property>
                    <property name="margin-start">24</property>
                    <property name="margin-end">24</property>
                    <property name="resize-toplevel">True</property>
                    <child>
                      <object class="GtkTextView" id="error_installing_textview">
                        <property name="can-focus">True</property>
                        <property name="editable">False</property>
                        <property name="monospace">True</property>
                      </object>
                    </child>
                    <child type="label">
                      <object class="GtkLabel">
                        <property name="can-focus">False</property>
                        <property name="label" translatable="yes">Details</property>
                      </object>
                    </child>
                  </object>
                  <packing>
                    <property name="expand">True</property>
                    <property name="fill">True</property>
                    <property name="position">2</property>
                  </packing>
                </child>
              </object>
              <packing>
                <property name="name">error-installing</property>
                <property name="position">3</property>
              </packing>
            </child>
          </object>
          <packing>
            <property name="expand">True</property>
            <property name="fill">True</property>
            <property name="position">2</property>
          </packing>
        </child>
      </object>
    </child>
  </template>
</interface>
